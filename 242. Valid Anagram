class Solution {
public:
    bool isAnagram(string s, string t) {
        int i=0;
        if (s.length() != t.length()) return false;
        unordered_map<char,int> freq;
        for(i=0;i<s.length();i++)
        {
            freq[s[i]-'a']++;
            freq[t[i]-'a']--;
        }
        for(i=0;i<26;i++)
        {
            if(freq[i]!=0) return false;
        }
        return true;
    }
};
